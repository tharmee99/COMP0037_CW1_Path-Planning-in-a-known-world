<launch>
  <!-- Note how the map is loaded. Use this to load other maps -->
  <arg name="map_file" default="$(find comp0037_resources)/maps/empty_terrain.yaml"/>
  <arg name="heuristic" default="0"/>  
  <rosparam command="load" file="$(find comp0037_resources)/maps/empty_poses.yaml" />
  <!--  -->
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>
  <node pkg="comp0037_planner_controller" required="true" output='screen' name="a_star_standalone_node" type="a_star_standalone_node.py" args="$(find comp0037_cw1)/exports/performanceMetrics.json $(arg heuristic)"/>
</launch>